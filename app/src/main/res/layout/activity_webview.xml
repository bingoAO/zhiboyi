<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context="com.bao.meijia.ui.activity.EvaluateListActivity">

    <include
        android:layout_alignParentTop="true"
        android:layout_height="@dimen/title_bar_height"
        android:layout_width="fill_parent"
        android:id="@+id/layout_title_bar_id"
        layout="@layout/title_bar" />

    <RelativeLayout
        android:id="@+id/webview_rl"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">

        <com.bao.wec.ui.customview.progressbar.CircularProgressBar
            android:layout_centerInParent="true"
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:indeterminate="true"
            app:cpb_color="@color/title_bar_color" />


        <com.bao.wec.ui.customview.MyWebView
            android:layout_alignParentTop="true"
            android:id="@+id/webview"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

        </com.bao.wec.ui.customview.MyWebView>

    </RelativeLayout>


</LinearLayout>
